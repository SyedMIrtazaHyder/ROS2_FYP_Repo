		<inertial>
			<mass value="0.5"/>
			<inertia ixx="0.0153" ixy="0" ixz="0" iyy="0.025" iyz="0" izz="0.0153"/>
		</inertial>
	</link>

	<!-- Front Left Wheel -->
	<joint name="front_left_wheel_joint" type="continuous">
		<axis xyz="0 0 1" />
		<origin xyz="0 0.125029 -0.025" rpy="-1.5707 0 0"/>
